

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">



        <!-- Begin Page Content -->
        <div class="container-fluid">

            <!-- Page Heading -->
            

            <!-- DataTales Example -->
            <div class="card shadow mb-4">
               
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                            <thead>
                                <tr>
                                    <th>No</th>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Phone</th>
                                    <th>Status</th>

                                </tr>
                            </thead>

                            <tbody>
                                {{#each userdetails}}
                                <tr>
                                    <td>{{inc @index}}</td>
                                    <td>{{this.name}}</td>
                                    <td>{{this.email}}</td>
                                    <td>{{this.phone}}</td>
                                    {{#if block}}
                                    <td><a class="btn btn-danger rounded-pill btn-sm" href="/admin/block/{{this._id}}">Block</a>
                                        {{else}}
                                    <td><a class="btn btn-success rounded-pill btn-sm" href="/admin/unblock/{{this._id}}">Unblock</a>

                                        {{/if}}
                                </tr>
                                {{/each}}

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
        <!-- /.container-fluid -->

    </div>
    <!-- End of Main Content -->

    <!-- Footer -->
    <footer class="sticky-footer bg-white">
        <div class="container my-auto">
            <div class="copyright text-center my-auto">
                <span>Copyright &copy; Your Website 2020</span>
            </div>
        </div>
    </footer>
    <!-- End of Footer -->

    </div>
    <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

   


    

</body>

</html>